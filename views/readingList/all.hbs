<h1>Your reading list</h1>


<ul>
{{#each this.links}}
    <li>
        <span>
            {{#if feed.faviconUrl}}
                <img src="{{feed.faviconUrl}}" alt="{{title}} favicon">
            {{/if}}
    <a href="{{url}}" target="_blank">{{title}}</a> - {{feed.title}}</span>
    </li>
{{else}}
    <span>Reading list empty \o/</span>
{{/each}}
</ul>

<a href="/">← Return to your feeds list</a>